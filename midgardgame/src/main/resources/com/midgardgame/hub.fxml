<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<VBox fx:id="hubContainer" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.midgardgame.HubController">
   <ImageView fx:id="hubTitle" fitWidth="1200.0" preserveRatio="true">
      <image>
         <Image url="@../images/HubImage/titleDef.png" />
      </image>
   </ImageView>
   <children>
      <Button fx:id="playButton" Text="" onAction="#switchToSecondary"/>
      <Button fx:id="settingsButton" Text="" onAction="#switchToSecondary"/>
      <Button fx:id="exitButton" Text="" onAction="#switchToSecondary"/>
      </children>
</VBox>


